	DLLFLAG="" \
	AS="gcc -Zomf" \
	ASFLAGS="-Di386" \
	LDFLAGS="-o ./" \
	LDFLAGS2="-Zstack 320 -Zsys -s" \
	OUT="-o" \
	OBJ=".obj" \
	CRC32="crc_gcc" \
	CRC32F="crc_gcc" \
	CRC32X="crc_gcc" \
	DEF="os2/unzip.def"

