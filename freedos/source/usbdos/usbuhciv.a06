;==============================================================================
;Syntax Help Screens
;==============================================================================

  ;----------------------------------------------------------------------------
  ;Syntax Help Strings (if user asks for, or otherwise needs, Help)
  ;----------------------------------------------------------------------------
  SyntaxMsg:
    DB  CR,'SYNTAX: '

  #IF !LITE
    DB 'USBUHCI '
  #ELSE
    DB 'USBUHCIL '
  #ENDIF

   DB '[Options]',CR,LF
   DB  LF
   DB '  /?      Õ Show this HELP screen',CR,LF
   DB '  /A      Õ Show all ALIASES for these command line Options',CR,LF
   DB '  /ErrLvl Õ Show all ErrorLevels (DOS Return Codes)',CR,LF
   DB '  /U      Õ UNINSTALL from memory',CR,LF
   DB '  /L:Y|N  Õ Force installation into LOW memory (default = No)',CR,LF
   DB  LF
   DB '  /Index #       Õ Use UHCI Host with PCI INDEX # (default = 0)',CR,LF
   DB '  /IOAddr #      Õ Use I/O base ADDR # (100h-FFE0h, divisible by 32)',CR,LF
   DB '  /IRQ #         Õ Use IRQ # (3-15, default = 9)',CR,LF
   DB '  /DisableLegacy Õ Disable Legacy Keyboard/Mouse support',CR,LF
   DB '  /DisableOther  Õ Disable Other Control program',CR,LF
   DB '  /P:#           Õ Pause # deciSeconds before finding Root Devices (0-255)',CR,LF
   DB '  /Beep:Y|N      Õ BEEP Speaker when bad Device is found (default = Yes)',CR,LF
   DB '  /PD[:#]        Õ Display Program Debug messages during install and uninstall',CR,LF
   DB '                      # = Milliseconds to delay each step (0-2000, default = 0)',CR,LF
   DB  LF
   DB '  /Run #     Õ Run (Resume/Enable) Device Address # ø',CR,LF
 #IF !LITE
   DB '  /Stop #    Õ Stop (Disable) Device Address #      ¥  0-1   = Host/Root Hub',CR,LF
   DB '  /Reset #   Õ Reset Device Address #               ¥  2-127 = Attached Dvc',CR,LF
   DB '  /Suspend # Õ Suspend Device Address #             ¥128-255 = All Hosts',CR,LF
 #ELSE
   DB '  /Stop #    Õ Stop (Disable) Device Address #      ¥ 0-1 =  Host/Root Hub',CR,LF
   DB '  /Reset #   Õ Reset Device Address #               ¥ 2-17  = Attached Dvc',CR,LF
   DB '  /Suspend # Õ Suspend Device Address #             ¥18-255 = All Hosts',CR,LF
 #ENDIF
   DB '  /Resume #  Õ Resume (not Enable) Device Address # Ÿ',CR,LF
   DB  LF
   DB '  /PowerOn #    Õ Power On Device Address #    ø',CR,LF
 #IF !LITE
   DB '  /PowerOff #   Õ Power Off Device Address #   ¥ 2-127',CR,LF
 #ELSE
   DB '  /PowerOff #   Õ Power Off Device Address #   ¥ 2-17',CR,LF
 #ENDIF
   DB '  /PowerReset # Õ Power Reset Device Address # Ÿ',CR,LF
   DB  LF
 #IF !LITE
   DB '  /SC:D# C# Õ Set Configuration of Device D# (2-127) to value C# (0-255)',CR,LF
   DB '  /RW:# Y|N Õ Enable/disable Remote Wakeup on Device # (2-127)',CR,LF
   DB '  /TM:# Y|N Õ Enable/disable Test Mode on Device # (2-127)',CR,LF
 #ELSE
