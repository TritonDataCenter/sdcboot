;==============================================================================
;Signature String & Version
;  MUST be at offset 0 in file!!
;==============================================================================
OvlSignature:
  DB 'USBUHCI'
 #IF LITE
  DB 'L'
 #ENDIF
  DB '.OVL'

OvlVerText:   ;Must be exactly 6 bytes long!
  DB ' 00.14'

StopTyping:
  DB EOF ;To stop the beeps and happy faces if the user TYPEs the file

Version:
  DW 000Eh ;Must match USBUHCI{L} version, NOT BCD
           ;Beginning w/ Version 0.12, when we started using AMIS,
           ;  this is no longer BCD.


