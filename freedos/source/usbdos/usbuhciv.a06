;==============================================================================
;ErrorLevel Tables & Strings
;==============================================================================

  ;----------------------------------------------------------------------------
  ;ErrorLevels, and Table for ErrorLevel Description Strings
  ;----------------------------------------------------------------------------
  ;Command-line/User problems
  ErLvlOption       EQU  1 ;Bad Option on the command line
  ErLvlStdIn        EQU  2 ;StdIn file is too big
  ErLvlLegacy       EQU  3 ;Legacy Support enabled, and user won't continue
  ErLvlAlreadyRun   EQU  4 ;Another driver has control of the USB Bus
  ;Memory/File problems
  ErLvlMemory       EQU  5 ;Memory problem with the computer
  ErLvlNotRealMode  EQU  6 ;Not in Real Mode or VCPI not installed
  ErLvlNoOvl        EQU  7 ;Could not find Overlay File
  ;Hardware/Compatibility problems
  ErLvlDOSVer       EQU  8 ;DOS Version too old
  ErLvlNo386        EQU  9 ;Not a 386+ CPU
  ErLvlNoPCI        EQU 10 ;No PCI Bus Installed
  ErLvlNoUHCI       EQU 11 ;No UHCI-compatible Controller found
  ErLvlNoStart      EQU 12 ;Host failed to Initialize Properly
  ;Program Install problems
  ErLvlTooManyHosts EQU 13 ;Too Many Hosts Already Installed
  ErLvlVersion      EQU 14 ;Another version of the program already installed
  ;Program Uninstall problems
  ErLvlNotInMem     EQU 15 ;Not in memory yet, can't perform Request
  ErLvlCantStopHost EQU 16 ;Could not stop the Host
  ErLvlNotFirstInst EQU 17 ;Not first Instance while Uninstalling
  ErLvlSharedIRQ    EQU 18 ;Another program is sharing the IRQ that we Enabled
  ErLvlDvc0Owner    EQU 19 ;Last Host, with Registered Device 0 Owners
  ;Internal problems
  ErLvlInternal     EQU 20 ;Internal processing error - shouldn't happen!

  ErrLvlHdr:
    DB  CR,LF
    DB '                         ERRORLEVELS (DOS RETURN CODES)',CR,LF
    DB '  ÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕÕ',CR,LF
    DB 0

  ErrLvlTbl:
    DB 0
       DB "No Error.",0
    DB ErLvlOption
       DB "Bad Command-line Option.",0
    DB ErLvlStdIn
       StdInTooBigMsg:
      #IF !LITE
       DB 'The Redirected Input Option File ("USBUHCI < {FileName}") is too big.',0
      #ELSE
       DB 'The Redirected Input Option File ("USBUHCIL < {FileName}") is too big.',0
      #ENDIF
    DB ErLvlLegacy
       DB "Legacy Support is installed, and the user wants to stop.",0
    DB ErLvlAlreadyRun
       DB "Another Driver is managing the USB Bus, and the user wants to stop.",0
    DB ErLvlMemory
       DB "Memory problem with the computer.",0
    DB ErLvlNotRealMode
       DB "The CPU must be in real mode, or a VCPI memory manager must be installed.",0
    DB ErLvlNoOvl
      #IF !LITE
       DB "Overlay File USBUHCI.OVL not found or incompatible.",0
      #ELSE
       DB "Overlay File USBUHCIL.OVL not found or incompatible.",0
      #ENDIF
    DB ErLvlDOSVer
       DOSVerMsg:
       DB "You must be using DOS version 3.00 or later to use this program.",0
    DB ErLvlNo386
       No386Msg:
       DB "The CPU in the computer must be at least an 80386.",0
    DB ErLvlNoPCI
       NoPCIMsg:
       DB "Could not find a PCI-compatible BIOS.",0
    DB ErLvlNoUHCI
       NoUHCIMsg:
       DB 'There is no UHCI-compatible USB Host Controller Installed.',0
    DB ErLvlNoStart
       DB "The USB Host Controller failed to Initialize properly.",0
    DB ErLvlTooManyHosts
       DB "There are too many Host Drivers already installed (maximum 16).",0
    DB ErLvlVersion
       DB "An incompatible USB Driver is already installed in memory.",0
    DB ErLvlNotInMem
      NotInMemMsg:
      #IF !LITE
       DB "USBUHCI is not installed in memory yet -- can't perform Request.",0
      #ELSE
       DB "USBUHCIL is not installed in memory yet -- can't perform Request.",0
      #ENDIF
    DB ErLvlCantStopHost
      DB "Unable to stop the Host Controller from running.",0
    DB ErLvlNotFirstInst
      #IF !LITE
       DB "Can't uninstall: An instance of USBUHCI was installed after this one.",0
      #ELSE
       DB "Can't uninstall: An instance of USBUHCIL was installed after this one.",0
      #ENDIF
    DB ErLvlSharedIRQ
       DB "Can't uninstall: Another program is sharing the IRQ that we enabled.",0
    DB ErLvlDvc0Owner
       DB "Can't uninstall: Another program is still registered as a Device0 Owner.",0
    DB ErLvlInternal
       InternalErrMsg:
       DB "Internal processing error - contact Bret Johnson!",0
    DB -1,-1 ;End of Table

