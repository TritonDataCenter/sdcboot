;CODE TO DETERMINE THE CURRENT DOS CODE PAGE
;北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北

;------------------------------------------------------------------------------
;GET THE CURRENT DOS SCREEN CODE PAGE
;Inputs:
;Outputs: CodePage
;Changes:
;------------------------------------------------------------------------------
GetCodePage:
  PUSH AX,BX,DX    ;Save used registers
  MOV  AX,0AD00h   ;Function AD80h (DISPLAY.SYS installed?)
  INT  2Fh         ;Do it
  OR   AL,AL       ;Is it installed?
  JZ  >P20         ;If not, try to get the Code Page some other way
P10:               ;DISPLAY.SYS installed
  MOV  AX,0AD02h   ;If so, function AD02h (Get Active Code Page)
  INT  2Fh         ;Do it
  JC  >P20         ;If none available, get the Code Page some other way
  MOV  CodePage,BX ;If we got it, store it
  JMP >P90         ;And we're done
P20:               ;DISPLAY.SYS not installed
  MOV  AX,6601h    ;Function 6601h (Get Code Page Information)
  STC              ;Preset the error flag (in case of bad DOS version)
  INT  21h         ;Do it (returns BX=Active Code Page, DX=System Code Page)
  JC  >P90         ;If there was an error, quit
  MOV  CodePage,BX ;If OK, store the Code Page
P90:               ;Done
  POP  DX,BX,AX    ;Restore used registers
  RET


