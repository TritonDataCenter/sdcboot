;CODE TO WRITE STRINGS AND NUMBERS TO THE SCREEN
;北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北

;------------------------------------------------------------------------------
;WRITE THE CONTENTS OF A WORD OR DWORD IN THE MINIMUM SPACE POSSIBLE (AS HEX)
;Inputs:  AL/AX/BP:AX = Byte/Word/DWord to Write
;Outputs: Writes to Screen
;Changes:
;NOTES: This writes a Byte, Word, or DWord, whichever is appropriate
;------------------------------------------------------------------------------
WriteDWordMinHex:
  OR   BP,BP           ;More than a Word can handle?
  JZ  >M30             ;If not, just handle it as a word
  TEST BP,0FF00h       ;Anything in the high byte?
  JNZ >M20             ;If so, write the whole DWord
M10:                   ;Need three bytes
  CALL WriteTriByteHex ;Write three Bytes
  JMP >M90             ;Done
M20:                   ;Need to whole DWord
  CALL WriteDWordHex   ;Write the DWord
  JMP >M90             ;Done

WriteWordMinHex:
M30:                   ;Need two bytes or less
  CMP  AX,0FFh         ;More than a Byte can handle?
  JA  >M40             ;If so, jump to write a Word
M35:                   ;Only needs a Byte
  CALL WriteByteHex    ;Write a Byte
  JMP >M90             ;Done
M40:                   ;Needs a Word
  CALL WriteWordHex    ;Write a Word
M90:                   ;Done
;  CALL WriteHexEnd     ;Write the 'h'
  RET

