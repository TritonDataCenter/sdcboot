;SUPPORT CODE TO SEARCH THROUGH AND/OR PRINT ONE OF THE LOOKUP TABLES
;北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北

;------------------------------------------------------------------------------
;TEST AND HANDLE A UNICODE CHARACTER IF IT'S STANDARD ASCII
;Inputs:  DS = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Set if Character is Standard ASCII
;              Writes Character
;            = Clear if Character is not Standard ASCII
;              Writes Nothing
;Changes:
;------------------------------------------------------------------------------
HandleASCII:
  OR   BP,BP   ;Plane 0?
  JNZ >A70     ;If not, it can't be Standard ASCII
  OR   AH,AH   ;Anything in the high byte?
  JNZ >A70     ;If so, it can't be Standard ASCII
  CMP  AL,20h  ;Standard ASCII?
  JB  >A70     ;If not, we're done
  CMP  AL,7Eh  ;Standard ASCII?
  JBE >A80     ;If so, handle it
A70:           ;Error (not Standard ASCII)
  STC          ;Set return flag
  JMP >A90     ;Done
A80:           ;OK (is Standard ASCII)
  CALL WriteAL ;Write it
  CLC          ;Set return flag
A90:           ;Done
  RET

;------------------------------------------------------------------------------
;TEST AND HANDLE A UNICODE CHARACTER IF IT'S A CONTROL CHARACTER
;Inputs:  DS = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Set if Character is a Control Character
;              Writes Correct Description String
;            = Clear if Character is not a Control Character
;              Writes Nothing
;Changes:
;------------------------------------------------------------------------------
HandleControl:
  PUSH BX,DX                ;Save used registers
  OR   BP,BP                ;Plane 0?
  JNZ >C70                  ;If not, we're done
  MOV  BX,ControlTbl00      ;Test for
  CALL SearchWord2StringTbl ;  a Control Character
  JC  >C70                  ;If not, we're done
C10:                        ;Is a Control Character
  MOV  DX,BX                ;If so, point [DX] at the description
  CMP  AX,2400h             ;Is it an Actual Control Char or just a Descr?
  JB  >C30                  ;If an actual Control Char, jump to handle it
  CMP  AX,2421h             ;Is it an Actual Control Char or just a Descr?
  JA  >C30                  ;If an actual Control Char, jump to handle it
C20:                        ;Is just a Control Characer Description
  CALL WriteItDX            ;Write the Description with no Brackets
  JMP >C90                  ;Done
C30:                        ;Is an Actual Control Char
  CALL WriteCurlyString     ;Write it
  JMP >C80                  ;Done
C70:                        ;Error (not a Control Character)
  STC                       ;Set return flag
  JMP >C90                  ;Done
C80:                        ;OK (is a Control Character)
  CLC                       ;Set return flag
C90:                        ;Done
  POP  BX,DX                ;Restore used registers
  RET

;------------------------------------------------------------------------------
;TEST AND HANDLE A UNICODE CHARACTER IF IT'S ILLEGAL (NOT A CHARACTER)
;Inputs:  DS = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Set if Character is Illegal
;              Writes Correct Description String
;            = Clear if Character is not Illegal
;              Writes Nothing
;Changes:
;NOTES: We don't need to build lookup tables for this, since there are only
;         a limited number of illegal characters and they follow a
;         distinct, easy-to-program pattern.
;------------------------------------------------------------------------------
HandleIllegal:
  PUSH DX               ;Save used registers
  CMP  AX,0FFFEh        ;Is it a "Universal" Illegal Character?
  JAE >I80              ;If so, handle it
  CMP  BP,10h           ;Is the high word too big to be legal?
  JA  >I80              ;If so, it's Illegal
  OR   BP,BP            ;If not, is the high DWord 0?
  JNZ >I70              ;If not, it's not Illegal
  CMP  AX,0FDD0h        ;Is it a Plane 0 Illegal Character?
  JB  >I70              ;If not, it's not Illegal
  CMP  AX,0FDEFh        ;Is it a Plane 0 Illegal Character?
  JBE >I80              ;If so, handle it
I70:                    ;Error (not an Illegal Character)
  STC                   ;Set return flag
  JMP >I90              ;Done
I80:                    ;OK (is an Illegal Character)
  MOV  DX,IllegalMsg    ;Point at the Illegal Description Message
  CALL WriteCurlyString ;Write the Number and Description
  CLC                   ;Set return flag
I90:                    ;Done
  POP  DX               ;Restore used registers
  RET

;------------------------------------------------------------------------------
;PROCESS A UNICODE CHARACTER IF IT'S UNUSED/PRIVATE/RESERVED/ETC.
;Inputs:  DS    = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Clear if OK (Conversion was performed)
;              Character is written to the screen
;            = Set if error (Character not Converted)
;              Writes Nothing
;Changes:
;------------------------------------------------------------------------------
HandleSpecial:
  PUSH BX,DX                  ;Save used registers
  MOV  DX,UnusedMsg           ;Assume Unused
  CALL TestUnused             ;Is it Unused?
  JNC >P80                    ;If so, handle it
  MOV  DX,ReservedMsg         ;Assume Reserved
  CALL TestReserved           ;Is it Reserved?
  JNC >P80                    ;If so, handle it
  MOV  DX,UnknownMsg          ;Assume Unknown
  CALL TestUnknown            ;Is it Unknown?
  JNC >P80                    ;If so, handle it
P10:                          ;Test Private
  MOV  DX,PrivateMsg          ;Assume Private
  OR   BP,BP                  ;Plane 0?
  JNE >P20                    ;If not, try other possibilities
  CMP  AX,0E000h              ;Private?
  JB  >P50                    ;If not, it's not Private
  CMP  AX,0F8FFh              ;Private?
  JBE >P80                    ;If so, handle it
P20:                          ;Test Private Plane 0Fh
  CMP  BP,0000Fh              ;Plane 0Fh?
  JE  >P30                    ;If so, handle it
  CMP  BP,00010h              ;Plane 10h?
  JNE >P50                    ;If not, try other possibilities
P30:                          ;Plane 0Fh or 10h
  CMP  AX,0FFFDh              ;Private?
  JBE >P80                    ;If so, handle it
P50:                          ;Test Surrogate
  OR   BP,BP                  ;Plane 0?
  JNZ >P70                    ;If not, it can't be a Surrogate
  MOV  DX,SurrogateMsg        ;Assume Surrogate
  MOV  BX,SurrogateRangeTbl00 ;Is it
  CALL SearchWordRangeTbl     ;  Surrogate?
  JNC >P80                    ;If so, handle it
P70:                          ;Error (no match)
  STC                         ;Set return flag
  JMP >P90                    ;Done
P80:                          ;OK (found a match)
  CALL WriteCurlyString       ;Write it
  CLC                         ;Set return flag
P90:                          ;Done
  POP  DX,BX                  ;Restore used registers
  RET

;------------------------------------------------------------------------------
;PROCESS A UNICODE CHARACTER IF IT'S "STANDARD"
;Inputs:  DS    = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Clear if OK (Conversion was performed)
;              Character is written to the screen
;            = Set if error (Character not Converted)
;              Writes Nothing
;Changes:
;------------------------------------------------------------------------------
HandleStandard:
  PUSH BX                 ;Save used registers
  CMP  BP,1               ;Which Plane?
  JB  >S00                ;If Plane 0, jump to handle it
  JE  >S10                ;If Plane 1, jump to handle it
  CMP  BP,2               ;Is it Plane 2?
  JE  >S20                ;If so, jump to handle it
  JMP >S70                ;If none of these, Error
S00:                      ;Plane 0
  MOV  BX,StandardTbl00   ;Test for
  CALL SearchWord2ByteTbl ;  a "Standard" Character
  JNC >S80                ;If so, we're done
  JMP >S70                ;If not, it's not Standard
S10:                      ;Plane 1
  MOV  BX,StandardTbl01   ;Test for
  CALL SearchWord2ByteTbl ;  a "Standard" Character
  JNC >S80                ;If so, we're done
  JMP >S70                ;If not, it's not Standard
S20:                      ;Plane 2
  MOV  BX,StandardTbl02   ;Test for
  CALL SearchWord2ByteTbl ;  a "Standard" Character
  JNC >S80                ;If so, we're done
S70:                      ;Error (not a "Standard" Character)
  STC                     ;Set return flag
  JMP >S90                ;Done
S80:                      ;OK (is a "Standard" Character)
  CALL WriteCharBX        ;Write the Character
  CLC                     ;Set return flag
S90:                      ;Done
  POP  BX                 ;Restore used registers
  RET

;------------------------------------------------------------------------------
;PROCESS A UNICODE CHARACTER IF IT'S A VARIATION SELECTOR
;Inputs:  DS    = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Clear if Character is a Variation Selector
;              Character is written to the screen
;            = Set if Character is not a Variation Selector
;              Writes Nothing
;Changes:
;------------------------------------------------------------------------------
HandleVariation:
  PUSH AX,DX           ;Save used registers
  CMP  BP,0000Eh       ;Plane E?
  JE  >V30             ;If so, jump to handle it
  OR   BP,BP           ;Plane 0?
  JNE >V70             ;If not, we're done
V10:                   ;Plane 0
  CMP  AH,0FEh         ;Variation Selector?
  JB  >V70             ;If not, we're done
  CMP  AL,00Fh         ;Variation Selector?
  JA  >V70             ;If not, we're done
V15:                   ;Is a Plane 0 Variation Selector
  SUB  AX,(0FE00h-1)   ;Convert AX to a Selector Number
  JMP >V50             ;COntinue
V30:                   ;Plane E
  CMP  AH,01h          ;Variation Selector?
  JNE >V70             ;If not, we're done
  CMP  AL,0EFh         ;Variation Selector?
  JA  >V70             ;If not, we're done
V35:                   ;Is a Plane E Variation Selector
  SUB  AX,(100h-17)    ;Convert AX to a Selector Number
V50:                   ;AX = Selector Number
  MOV  DX,VariationMsg ;Write the
  CALL WriteItDX       ;  Header
  CALL WriteWordMin    ;Write the Selector Number
  CALL WriteCurlyRight ;Write the Footer
  JMP >V80             ;Done
V70:                   ;Error (not a Variatioon Selector)
  STC                  ;Set return flag
  JMP >V90             ;Done
V80:                   ;OK (is a Variation Selector)
  CLC                  ;Set return flag
V90:                   ;Done
  POP  DX,AX           ;Restore used registers
  RET

;------------------------------------------------------------------------------
;PROCESS A UNICODE CHARACTER IF IT'S A TAG
;Inputs:  DS    = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Clear if Character is a Tag
;              Character is written to the screen
;            = Set if Character is not a Tag
;              Writes Nothing
;Changes:
;------------------------------------------------------------------------------
HandleTag:
  PUSH AX,BX,DX        ;Save used registers
  CMP  BP,0000Eh       ;Plane E?
  JNE >T70             ;If not, we're done
  OR   AH,AH           ;Tag?
  JNZ >T70             ;If not, we're done
  MOV  DX,TagMsg       ;Point at the Header
  MOV  BX,LanguageMsg  ;Assume Language Tag
  CMP  AL,1            ;Language Tag?
  JE  >T20             ;If so, handle it
  MOV  BX,SpaceWMsg    ;Assume Space Tag
  CMP  AL,20h          ;Space Tag?
  JB  >T70             ;If not, we're done
  JE  >T20             ;If so, handle it
  MOV  BX,CancelMsg    ;Assume Cancel Tag
  CMP  AL,07Fh         ;Cancel Tag?
  JA  >T70             ;If not, we're done
  JB  >T30             ;If not, continue
T20:                   ;Tag requires a written descriptor
  CALL WriteItDX       ;Write the Header
  CALL WriteItBX       ;Write the Description
  JMP >T80             ;Done
T30:                   ;Tag uses a single character
  CALL WriteItDX       ;Write the Header
  CALL WriteAL         ;Write the Character
  JMP >T80             ;Done
T70:                   ;Error (not a Variation Selector)
  STC                  ;Set return flag
  JMP >T90             ;Done
T80:                   ;OK (is a Variation Selector)
  CALL WriteCurlyRight ;Write the Footer
  CLC                  ;Set return flag
T90:                   ;Done
  POP  DX,BX,AX        ;Restore used registers
  RET

;------------------------------------------------------------------------------
;PROCESS A UNICODE CHARACTER IF IT'S A STRING
;Inputs:  DS    = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Clear if Character is a Tag
;              Character is written to the screen
;            = Set if Character is not a Tag
;              Writes Nothing
;Changes:
;------------------------------------------------------------------------------
HandleString:
  PUSH BX                   ;Save used registers
  MOV  BX,StringTbl00       ;Assume Plane 0
  CMP  BP,1                 ;Which Plane is it?
  JB  >S30                  ;If Plane 0, continue
  MOV  BX,StringTbl01       ;Assume Plane 1
  JE  >S30                  ;If Plane 1, continue
  MOV  BX,StringTbl02       ;Assume Plane 2
  CMP  BP,2                 ;Is it Plane 2?
  JA  >S70                  ;If none of these, Error
S30:                        ;[BX] = String Table to Search
  CALL SearchWord2StringTbl ;Search the Table
  JNC  >S80                 ;If so, write it
S70:                        ;Error (not a Variatioon Selector)
  STC                       ;Set return flag
  JMP >S90                  ;Done
S80:                        ;OK (is a Variation Selector)
  CALL WriteItBX            ;Write the String
  CLC                       ;Set return flag
S90:                        ;Done
  POP  BX                   ;Restore used registers
  RET

;------------------------------------------------------------------------------
;TEST A UNICODE CHARACTER TO SEE IF IT'S UNUSED
;Inputs:  DS = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Set if Character is Unused
;            = Clear if Character is not Illegal
;Changes:
;------------------------------------------------------------------------------
TestUnused:
  PUSH BX                   ;Save used registers
  OR   BP,BP                ;Plane 0?
  JNZ >U10                  ;If not, handle other possibilities
U00:                        ;Plane 0
  MOV  BX,UnusedSingleTbl00 ;Is it
  CALL SearchWordTbl        ;  Unused?
  JNC >U80                  ;If so, handle it
  MOV  BX,UnusedRangeTbl00  ;Is it
  CALL SearchWordRangeTbl   ;  Unused?
  JNC >U80                  ;If so, handle it
  JMP >U70                  ;If not, we're done
U10:                        ;Not Plane 0
  CMP  BP,1                 ;Plane 1?
  JNE >U20                  ;If not, handle other possibilities
  MOV  BX,UnusedSingleTbl01 ;Is it
  CALL SearchWordTbl        ;  Unused?
  JNC >U80                  ;If so, handle it
  MOV  BX,UnusedRangeTbl01  ;Is it
  CALL SearchWordRangeTbl   ;  Unused?
  JNC >U80                  ;If so, handle it
  JMP >U70                  ;If not, we're done
U20:                        ;Not Plane 1
  CMP  BP,2                 ;Plane 2?
  JNE >U30                  ;If not, handle other possibilities
  MOV  BX,UnusedRangeTbl02  ;Is it
  CALL SearchWordRangeTbl   ;  Unused?
  JNC >U80                  ;If so, handle it
  JMP >U70                  ;If not, we're done
U30:                        ;Not Plane 2
  CMP  BP,0000Eh            ;Plane 0Eh?
  JNE >U40                  ;If not, handle other possibilities
  MOV  BX,UnusedRangeTbl0E  ;Is it
  CALL SearchWordRangeTbl   ;  Unused?
  JNC >U80                  ;If so, handle it
  JMP >U70                  ;If not, we're done
U40:                        ;Not Plane 0Eh
  CMP  BP,0Fh               ;Is it Plane 0Fh or 10h?
  JAE >U70                  ;If so, there are no unused characters
U50:                        ;Not a "Special" Plane
  CMP  AX,0FF80h            ;Too small to be Unused?
  JB  >U70                  ;If so, it's not Unused
  CMP  AX,0FFFDh            ;Too big to be Unused?
  JBE >U80                  ;If not, it's Unused
U70:                        ;Error (not an Unused Character)
  STC                       ;Set return flag
  JMP >U90                  ;Done
U80:                        ;OK (is an Unused Character)
  CLC                       ;Set return flag
U90:                        ;Done
  POP  BX                   ;Restore used registers
  RET

;------------------------------------------------------------------------------
;TEST A UNICODE CHARACTER TO SEE IF IT'S RESERVED
;Inputs:  DS = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Set if Character is Reserved
;            = Clear if Character is not Reserved
;Changes:
;------------------------------------------------------------------------------
TestReserved:
  PUSH BX                     ;Save used registers
  OR   BP,BP                  ;Plane 0?
  JNZ >V10                    ;If not, handle other possibilities
V00:                          ;Plane 0
  MOV  BX,ReservedSingleTbl00 ;Is it
  CALL SearchWordTbl          ;  Reserved?
  JNC >V80                    ;If so, handle it
  MOV  BX,ReservedRangeTbl00  ;Is it
  CALL SearchWordRangeTbl     ;  Reserved?
  JNC >V80                    ;If so, handle it
  JMP >V70                    ;If not, we're done
V10:                          ;Not Plane 0
  CMP  BP,1                   ;Plane 1?
  JNE >V70                    ;If not, we're done
  MOV  BX,ReservedSingleTbl01 ;Is it
  CALL SearchWordTbl          ;  Reserved?
  JNC >V80                    ;If so, handle it
  MOV  BX,ReservedRangeTbl01  ;Is it
  CALL SearchWordRangeTbl     ;  Reserved?
  JNC >V80                    ;If so, handle it
V70:                          ;Error (not a Reserved Character)
  STC                         ;Set return flag
  JMP >V90                    ;Done
V80:                          ;OK (is a Reserved Character)
  CLC                         ;Set return flag
V90:                          ;Done
  POP  BX                     ;Restore used registers
  RET

;------------------------------------------------------------------------------
;TEST A UNICODE CHARACTER TO SEE IF IT'S UNKNOWN
;Inputs:  DS = Local Data Area
;         BP:AX = UniCode Character to Test
;Outputs: CF = Set if Character is Unknown
;            = Clear if Character is not Unknownd
;Changes:
;------------------------------------------------------------------------------
TestUnknown:
  PUSH BX                   ;Save used registers
  OR   BP,BP                ;Plane 0?
  JNZ >K10                  ;If not, handle other possibilities
K00:                        ;Plane 0
  MOV  BX,UnknownRangeTbl00 ;Is it
  CALL SearchWordRangeTbl   ;  Unkown?
  JNC >K80                  ;If so, handle it
  JMP >K70                  ;If not, we're done
K10:                        ;Not Plane 0
  CMP  BP,1                 ;Plane 1?
  JNE >K20                  ;If not, handle other possibilities
  MOV  BX,UnknownRangeTbl01 ;Is it
  CALL SearchWordRangeTbl   ;  Unknown?
  JNC >K80                  ;If so, handle it
  JMP >K70                  ;If not, we're done
K20:                        ;Not Plane 1
  CMP  BP,2                 ;Plane 2?
  JNE >K30                  ;If not, handle other possibilities
  MOV  BX,UnknownRangeTbl02 ;Is it
  CALL SearchWordRangeTbl   ;  Unknown?
  JNC >K80                  ;If so, handle it
  JMP >K70                  ;If not, we're done
K30:
  CMP  BP,0Eh               ;Plane E?
  JNE >K40                  ;If not, handle other possibilities
  MOV  BX,UnknownRangeTbl0E ;Is it
  CALL SearchWordRangeTbl   ;  Unknown?
  JNC >K80                  ;If so, handle it
  JMP >K70                  ;If not, we're done
K40:
  CMP  BP,0Fh               ;Plane 0Fh or 10h?
  JAE >K70                  ;If so, there are no unknown characters
K50:                        ;Not a "Special" Plane
  MOV  BX,0FF7Fh            ;Assume Planes 3h-Dh
  CMP  BP,0Dh               ;Planes 3h-Dh?
  JBE >K55                  ;If so, continue
  MOV  BX,0FFFDh            ;All other Planes
K55:                        ;BX contains Generic Unknown High Range
  CMP  AX,BX                ;Is it Unknown?
  JBE >K80                  ;If so, handle it
K70:                        ;Error (not an Unknown Character)
  STC                       ;Set return flag
  JMP >K90                  ;Done
K80:                        ;OK (is an Unknown Character)
  CLC                       ;Set return flag
K90:                        ;Done
  POP  BX                   ;Restore used registers
  RET

;------------------------------------------------------------------------------
;PROCESS A UNICODE CHARACTER LOOKING FOR A CODE PAGE-SPECIFIC CONVERSION
;Inputs:  AX = UniCode Character
;         DS = Local Data Area
;         CodePage already filled in (current DOS Code Page)
;Outputs: CF = Clear if OK (Conversion was performed)
;              Character is written to the screen
;            = Set if error (Character not Converted)
;Changes:
;------------------------------------------------------------------------------
HandleCodePage:
  PUSH AX,BX,SI           ;Save used registers
  MOV  SI,CodePageTbl00   ;Assume Plane 0
  CMP  BP,1               ;Which Plane is it?
  JB  >P10                ;If Plane 0, jump to handle it
  MOV  SI,CodePageTbl01   ;Assume Plane 1
  JE  >P10                ;If Plane 1, jump to handle it
  MOV  SI,CodePageTbl02   ;Assume Plane 2
  CMP  BP,2               ;Is it Plane 2?
  JNE >P70                ;If not, Error
P10:                      ;[SI] = Code Page Table
  MOV  BX,CodePage        ;BX = Current DOS Code Page
P20:                      ;Loop to here for each Table Entry
  CMP  W [SI],-1          ;End of the Table?
  JE  >P70                ;If so, error
  CMP  BX,[SI]            ;Is it the Code Page we're looking for?
  JE  >P30                ;If so, handle it
  ADD  SI,4               ;If not, point at the next table entry
  JMP  P20                ;Keep looking
P30:                      ;Found a Code Page match
  MOV  BX,[SI+2]          ;Point [BX] at the Code Page conversion Table
  CALL SearchWord2ByteTbl ;Look for the Unicode character in the Table
  JNC >P80                ;If it's there, jump to handle it
P70:                      ;Error (Character not found)
  STC                     ;Set return flag
  JMP >P90                ;Done
P80:                      ;OK (Character found)
  CALL WriteCharBX        ;Write the Translated Character
  CLC                     ;Set return flag

;  call WriteSpace
;  call WriteSpace
;  stc

P90:                      ;Done
  POP  SI,BX,AX           ;Restore used registers
  RET

;------------------------------------------------------------------------------
;WRITE A CHARACTER AND DESCRIPTION STRING SURROUNDED BY CURLY BRACKETS
;Inputs:   DS    = Local Data Area
;          BP:AX = Unicode Character to Write
;         [DX]   = Description String to Write
;Outputs: To Screen
;CHanges:
;------------------------------------------------------------------------------
WriteCurlyString:
  CALL WriteCurlyLeft   ;Write the Left Curly Bracket
  CALL WriteDWordMinHex ;Write the Number (Minimum Length)
  CALL WriteEquals      ;Write an Equals
  CALL WriteItDX        ;Write the Description
  CALL WriteCurlyRight  ;Write the Right Curly Bracket
  RET

;------------------------------------------------------------------------------
;SEARCH THROUGH A WORD-TO-BYTE OR WORD-TO-STRING TABLE FOR A MATCH
;Inputs:  CS = Local Data Area
;         AX = Word to find in the Table
;         CS:[BX] = Table to Search
;         CLD already issued
;Outputs: CF = Clear if OK (Found in the Table)
;             [BX] = Pointer to Character or String
;            = Set if Error (Not Found in the Table)
;              BX = Unchanged
;Changes:
;------------------------------------------------------------------------------
SearchWord2ByteTbl:
  PUSH CX        ;Save used registers
  XOR  CL,CL     ;Mark as a Byte Search
  JMP >Y00       ;Do it
SearchWord2StringTbl:
  PUSH CX        ;Save used registers
  MOV  CL,-1     ;Mark as a String Search
;  JMP >Y00       ;Do it
Y00:
  PUSH AX,DX,SI  ;Save used registers
  PUSH DS        ;Save used registers
  MOV  DX,AX     ;Save original AX
  MOV  DS,CS     ;Point DS:[SI]
  MOV  SI,BX     ;  at the Table
Y10:             ;Loop to here for each Table Entry
  CMP  W [SI],-1 ;End of the Table?
  JE  >Y70       ;If so, we're done
Y20:             ;Loop to here for each Wword in the Table Entry
  LODSW          ;Get the next Word from the Table
  CMP  AX,-1     ;Is it the end of the Table Entry?
  JZ  >Y30       ;If so, handle it
  CMP  AX,DX     ;Is it the one we're looking for?
  JE  >Y40       ;If so, handle it
  JMP  Y20       ;If not, keep looking
Y30:             ;Not in this entry
  LODSB          ;Get the first byte (of the string)
  OR  CL,CL      ;Are we doing a Byte or a String?
  JZ  Y10        ;If a Byte, we're done with this entry
  OR  AL,AL      ;If a String, is it the end of the string?
  JNZ Y30        ;If not, keep looking
  JMP  Y10       ;Keep looking
Y40:             ;It's a match!
  LODSW          ;Find the
  CMP  AX,-1     ;  last word of
  JNE  Y40       ;  the Table Entry
  MOV  BX,SI     ;Point [BX] at the Byte or String
  JMP >Y80       ;Done
Y70:             ;Error (not found)
  STC            ;Set return flag
  JMP >Y90       ;Done
Y80:             ;OK
  CLC            ;Set return flag
Y90:             ;Done
  POP  DS        ;Restore used registers
  POP  SI,DX,AX  ;Restore used registers
  POP  CX        ;Restore used registers
  RET

;------------------------------------------------------------------------------
;SEARCH THROUGH A WORD OR WORD-RANGE TABLE FOR A MATCH
;Inputs:   AX = Word to find in the Table
;         [BX] = Table to Search
;          CLD already issued
;Outputs: CF = Clear if OK (Found in the Table)
;            = Set if Error (Not Found in the Table)
;Changes:
;------------------------------------------------------------------------------
SearchWordRangeTbl:
  PUSH CX        ;Save used registers
  MOV  CL,-1     ;Mark as Ranges
  JMP >G00       ;Do it
SearchWordTbl:
  PUSH CX        ;Save used registers
  XOR  CL,CL     ;Mark as single entries
;  JMP >G00       ;Do it
G00:
  PUSH SI        ;Save used registers
  MOV  SI,BX     ;  at the Table
G10:             ;Loop to here for each Table Entry
  CMP  W [SI],-1 ;End of the Table?
  JE  >G70       ;If so, no match
  OR   CL,CL     ;Doing Ranges or Single Values?
  JNZ >G30       ;If Ranges, jump to handle it
G20:             ;Doing Single Values
  CMP  AX,[SI]   ;Is it a match?
  JNE >G40       ;If not, continue
  JMP >G80       ;If so, it's a match
G30:             ;Doing Ranges
  INC  SI,2      ;Point at the upper word of the range
  CMP  AX,[SI-2] ;Is it below our range?
  JB  >G40       ;If so, no match
  CMP  AX,[SI]   ;Is it within our range?
  JBE >G80       ;If so, it's a match
G40:             ;Done with this Table Entry
  INC  SI,2      ;Point at the next Table Entry
  JMP  G10       ;Keep searching
G70:             ;Error (not in Table)
  STC            ;Set return flag
  JMP >G90       ;Done
G80:             ;OK (in Table)
  CLC            ;Set return flag
G90:             ;Done
  POP  SI        ;Restore used registers
  POP  CX        ;Restore used registers
  RET


;北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北
;CODE TO TEST THE COMPUTER FOR MINIMUM COMPATIBILITY (DOS VERSION, ETC)
;北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北北

;------------------------------------------------------------------------------
;TEST THE COMPUTER TO SEE IF IT'S COMPATIBLE (CPU, DOS VERSION, WHATEVER)
;Inputs:  InMemory
;Outputs: Quits the program if Computer is Incompatible
;         Returns if Computer is OK
;         Stores all kinds of values in memory variables
;Changes:
;------------------------------------------------------------------------------
TestCompatibility:
  PUSH AX,DX          ;Save used registers
  MOV  AL,ErLvlDOSVer ;Assume incorrect
  MOV  DX,DOSVerMsg   ;  DOS Version
  CALL TestDOSVer     ;  new enough?
  JNC >C90            ;If not, quit
C70:                  ;The computer is incompatible for some reason
  JMP  Exit           ;Quit!
C90:                  ;The computer is compatible!
  POP  DX,AX          ;Restore used registers
  RET

;------------------------------------------------------------------------------
;TEST THE CURRENT DOS VERSION
;Inputs:  MinDOSVer (EQUate)
;Outputs: CF = Clear if DOS version >= MinDOSVer
;         CF = Set if DOS version < MinDOSVer
;Changes:
;------------------------------------------------------------------------------
TestDOSVer:
  PUSH AX,BX,CX     ;Save used registers
  MOV  AX,3000h     ;Service 30h (get DOS version number)
                    ;  DOS 5.0 requires AL = 00h, may change BX and CX
  INT  21h          ;Do it (Returns AL= Major, AH = Minor)
  XCHG AL,AH        ;Swap Major and Minor versions for testing
  CMP  AX,MinDOSVer ;Is the version new enough?
  JAE >D80          ;If so, it's OK
D70:                ;DOS is older than the minimum
  STC               ;Set the "too old" flag
  JMP >D90          ;We're done
D80:                ;DOS is >= the minimum
  CLC               ;Set the OK flag
D90:                ;We're done
  POP  CX,BX,AX     ;Restore used registers
  RET


